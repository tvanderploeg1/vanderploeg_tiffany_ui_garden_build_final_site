 "scripts": {
  "start": "react-scripts start",
  "build": "react-scripts build",
  "test": "react-scripts test --watchAll=false",
  "test:watch": "react-scripts test",
  "eject": "react-scripts eject",
  "storybook": "storybook dev -p 6006",
  "build-storybook": "storybook build",
  "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
  "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
  "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
  "prettier:check": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,md}",
  "prepare": "husky install",
  "pre-commit": "npm run prettier:check && npm run lint && npm test"
},
"eslintConfig": {
  "extends": [
    "react-app",
    "react-app/jest",
    "plugin:storybook/recommended"
  ],
  "rules": {
    "no-unused-vars": "error",
    "no-console": "warn"
  }
},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/preset-create-react-app": "^9.1.3",
    "@storybook/react-webpack5": "^9.1.3",
    "@testing-library/jest-dom": "^6.8.0",
    "eslint-plugin-storybook": "^9.1.3",
    "prop-types": "^15.8.1",
    "storybook": "^9.1.3",
    "webpack": "^5.101.3",
    "eslint": "^8.23.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "react-router-dom": "^6.8.0"
  }
